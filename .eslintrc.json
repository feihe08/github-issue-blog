{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "globals": {
        "atom": false
    },
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "no-console": 0
    }
}
